mapscripts LittlerootTown_MaysHouse_2F_MapScripts {}

script LittlerootTown_MaysHouse_2F_EventScript_PC {
	if (flag(FLAG_TEMP_1)) { msgbox(LittlerootTown_MaysHouse_2F_Text_EMailAlreadyRead, MSGBOX_SIGN) }
	else {
		msgbox(LittlerootTown_MaysHouse_2F_Text_PokemonTrainerSchoolEmail, MSGBOX_SIGN)
		setflag(FLAG_TEMP_1)
	}
	end
}

script LittlerootTown_MaysHouse_2F_EventScript_PichuDoll {
	msgbox(LittlerootTown_MaysHouse_2F_Text_PichuDoll, MSGBOX_SIGN)
	end
}
